#include <stdio.h>
#include <stdlib.h>
#include <time.h>


int main() {

char Adjour[] = "cette époustouflante";

int Nbj = 24;

int Nbm = 02;

int Nba = 2021;

char NomP[5];
NomP[0] = "de l'enfant surnommé \"l'idiot suicidaire\" \nà cause de son obsession aveugle pour l'éradication des titans.";
NomP[1] = 0;

printf("\n En %s journée du %d/%d/%d, nous allâmes vous raconter l'histoire %s \n", Adjour, Nbj, Nbm, Nba, NomP); 


printf("\n");

int my_rand (void);

            
            static int first = 0;
            
            if (first == 0)
            {
                srand (time (NULL));
                first = 1;
            }

   int i;

 printf("   Nombres pseudoaléatoires : \n \n ");
   for (i = 0; i<10; i++)
   {
     printf ("%d ", my_rand());
   }
   return (EXIT_SUCCESS);
}

int my_rand (void)
{
   return (rand ());
} 
